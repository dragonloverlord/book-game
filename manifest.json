{
  "manifest_version": 2,
  "name": "Book Writer",
  "version": "2.2.2.2",
  "icons": {
    "16": "images/Book-icon.png",
    "48": "images/Book-icon.png",
    "128": "images/Book-icon.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "storage",
    "contextMenus",
    "unlimitedStorage",
    "alwaysOnTopWindows"
    ]
}